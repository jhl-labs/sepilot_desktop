{
  "name": "sepilot-desktop",
  "version": "0.6.5",
  "description": "SEPilot Desktop - LLM Desktop Application with LangGraph, RAG, and MCP",
  "main": "dist/electron/electron/main.js",
  "scripts": {
    "postinstall": "node scripts/copy-monaco.js && electron-rebuild -f -w node-pty || true",
    "dev": "npm run clean:dev && npm run dev:electron-build-once && concurrently -n \"next,tsc,electron\" -c \"cyan,yellow,green\" \"npm run dev:next\" \"npm run dev:electron-build\" \"npm run dev:electron\"",
    "dev:next": "next dev --webpack",
    "clean:dev": "node scripts/clean-dev.js",
    "dev:electron-build-once": "tsc -p electron/tsconfig.json",
    "dev:electron-build": "tsc -p electron/tsconfig.json --watch --preserveWatchOutput",
    "dev:electron": "wait-on -d 1000 http://localhost:3000 && electron .",
    "build": "next build --webpack && pnpm run build:electron",
    "build:electron": "tsc -p electron/tsconfig.json",
    "build:icon": "node scripts/build-icon.js",
    "build:app": "pnpm run build && electron-builder",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:win": "pnpm run build && electron-builder --win",
    "build:linux": "pnpm run build && electron-builder --linux",
    "package": "pnpm run build && electron-builder --dir",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:frontend": "jest --selectProjects frontend",
    "test:backend": "jest --selectProjects backend",
    "test:watch": "jest --watch",
    "test:watch:frontend": "jest --selectProjects frontend --watch",
    "test:watch:backend": "jest --selectProjects backend --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:frontend": "jest --selectProjects frontend --coverage",
    "test:coverage:backend": "jest --selectProjects backend --coverage",
    "test:e2e": "playwright test --config=e2e_tests/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e_tests/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=e2e_tests/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=e2e_tests/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report e2e_tests/test-results/html-report",
    "depcruise": "depcruise app components lib electron --config .dependency-cruiser.js",
    "depcruise:graph": "depcruise app components lib electron --include-only '^(app|components|lib|electron)' --output-type dot | dot -T svg > dependency-graph.svg",
    "prepare": "husky"
  },
  "dependencies": {
    "@langchain/core": "^1.1.5",
    "@langchain/langgraph": "^1.0.4",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/dompurify": "^3.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/turndown": "^5.0.6",
    "@vscode/ripgrep": "^1.17.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "electron-serve": "^3.0.0",
    "eventsource": "^4.1.0",
    "https-proxy-agent": "^7.0.6",
    "i18next": "^25.7.2",
    "lucide-react": "^0.555.0",
    "mammoth": "^1.11.0",
    "markdown-to-jsx": "9.3.5",
    "mermaid": "^11.12.2",
    "module-alias": "^2.2.3",
    "monaco-editor": "^0.55.1",
    "nanoid": "^5.1.6",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "node-pty": "1.1.0-beta39",
    "pdf-parse": "^2.4.5",
    "plotly.js-dist-min": "^3.3.0",
    "pptxgenjs": "^4.0.1",
    "proxy-agent": "^6.5.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-i18next": "^15.1.2",
    "react-plotly.js": "^2.6.0",
    "react-syntax-highlighter": "^16.1.0",
    "sharp": "^0.34.5",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^3.4.0",
    "tiktoken": "^1.0.22",
    "turndown": "^7.2.2",
    "ws": "^8.18.3",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.10",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eventsource": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/path-browserify": "^1.0.3",
    "@types/plotly.js": "^3.0.8",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-plotly.js": "^2.6.3",
    "@types/sql.js": "^1.4.9",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "app-builder-bin": "5.0.0-alpha.6",
    "autoprefixer": "^10.4.22",
    "backstopjs": "^6.3.25",
    "buffer": "^6.0.3",
    "canvas": "^3.2.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.1.0",
    "dependency-cruiser": "^17.3.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-builder-squirrel-windows": "26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.0.10",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "node-gyp": "^12.1.0",
    "path-browserify": "^1.0.1",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "process": "^0.11.10",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "util": "^0.12.5",
    "wait-on": "^9.0.3",
    "webpack-bundle-analyzer": "^5.0.1"
  },
  "_moduleAliases": {
    "@": "dist/electron"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "app-builder-bin": "5.0.0-alpha.6",
      "tmp": "^0.2.4"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
