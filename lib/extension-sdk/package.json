{
  "name": "@sepilot/extension-sdk",
  "version": "0.2.0",
  "description": "Core SDK for SEPilot Desktop Extension development",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.mjs",
      "require": "./dist/ui/index.js",
      "default": "./dist/ui/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "default": "./dist/types/index.mjs"
    },
    "./ipc": {
      "types": "./dist/ipc/index.d.ts",
      "import": "./dist/ipc/index.mjs",
      "require": "./dist/ipc/index.js",
      "default": "./dist/ipc/index.mjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.js",
      "default": "./dist/runtime/index.mjs"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.mjs",
      "require": "./dist/agent/index.js",
      "default": "./dist/agent/index.mjs"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.mjs",
      "require": "./dist/store/index.js",
      "default": "./dist/store/index.mjs"
    },
    "./chat": {
      "types": "./dist/chat/index.d.ts",
      "import": "./dist/chat/index.mjs",
      "require": "./dist/chat/index.js",
      "default": "./dist/chat/index.mjs"
    },
    "./host": {
      "types": "./dist/host/index.d.ts",
      "import": "./dist/host/index.mjs",
      "require": "./dist/host/index.js",
      "default": "./dist/host/index.mjs"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js",
      "default": "./dist/services/index.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.mjs"
    },
    "./hooks/use-langgraph-stream": {
      "types": "./dist/hooks/use-langgraph-stream.d.ts",
      "import": "./dist/hooks/use-langgraph-stream.mjs",
      "require": "./dist/hooks/use-langgraph-stream.js",
      "default": "./dist/hooks/use-langgraph-stream.mjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && tsc src/index.ts src/agent/index.ts src/services/index.ts src/store/index.ts src/chat/index.ts src/host/index.ts src/hooks/index.ts src/ipc/index.ts src/runtime/index.ts src/ui/index.ts src/utils/index.ts src/types/index.ts --declaration --emitDeclarationOnly --outDir dist --target ES2020 --module ESNext --moduleResolution bundler --jsx react-jsx --skipLibCheck",
    "build:js": "tsup",
    "build:types": "tsc --project tsconfig.build.json",
    "dev": "tsup --watch",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "sepilot",
    "extension",
    "sdk"
  ],
  "author": "SEPilot Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": ">=0.263.0",
    "react-i18next": ">=13.0.0",
    "sonner": ">=1.0.0",
    "tailwind-merge": ">=2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "archiver": "^7.0.1",
    "@eslint/compat": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "ui": [
        "./dist/ui/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ],
      "ipc": [
        "./dist/ipc/index.d.ts"
      ],
      "runtime": [
        "./dist/runtime/index.d.ts"
      ],
      "agent": [
        "./dist/agent/index.d.ts"
      ],
      "store": [
        "./dist/store/index.d.ts"
      ],
      "chat": [
        "./dist/chat/index.d.ts"
      ],
      "host": [
        "./dist/host/index.d.ts"
      ],
      "services": [
        "./dist/services/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "hooks/use-langgraph-stream": [
        "./dist/hooks/use-langgraph-stream.d.ts"
      ]
    }
  }
}
